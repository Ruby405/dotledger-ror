<div class="page-header">
  <h1>Import Statement <small><%- accountName %></small></h1>
</div>

<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="import_file" class="col-lg-2 control-label">File</label>
    <div class="col-lg-5">
      <input type="file" id="import_file" name="file" />
    </div>
    <div class="errors col-lg-5"></div>
  </div>
  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <button type="submit" data-loading-text="Importing..." class="save btn btn-default">Import</button>
      <a class="btn btn-link" href="/accounts/<%= accountId %>">Cancel</a>
    </div>
  </div>
</form>
